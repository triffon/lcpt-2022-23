{
    "name": "Minlog",
    "build": {
	"dockerfile": "Dockerfile"
    },

    "settings": {
        "magicRacket.languageServer.command": "xvfb-racket",
        "magicRacket.REPL.arguments": [
            "--lib",
            "r5rs",
            "--repl"
        ],
        "magicRacket.languageServer.arguments": [
            "--lib",
            "racket-langserver",
            "--lib",
            "r5rs"
        ]
    },

    "extensions": [
        // Racket support
        "evzen-wybitul.magic-racket",
        // Bulgarian support
        "ms-ceintl.vscode-language-pack-bg"
    ],
}
